{
  "name": "SuperAdaCompanyApp",
  "author": "Futurice",
  "description": "React Native App Starter Kit for Android and iOS",
  "license": "MIT",
  "homepage": "http://getpepperoni.com",
  "bugs": "https://github.com/futurice/pepperoni-app-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/futurice/pepperoni-app-kit"
  },
  "keywords": [
    "React Native",
    "Starter Kit",
    "Bitrise",
    "Redux",
    "Auth0"
  ],
  "version": "1.1.5",
  "private": false,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "bundle:android": "./bundle_android.sh",
    "bundle:ios": "node ./node_modules/react-native/local-cli/cli.js bundle --platform ios --entry-file index.ios.js --bundle-output ios/SuperAda/main.jsbundle --dev=false --verbose",
    "test": "mocha src/**/*.spec.js",
    "test:watch": "chokidar 'src/**/*.js' 'test/**/*.js' -c 'npm test'",
    "lint": "eslint src test",
    "coverage": "rimraf coverage && istanbul cover --i=src/**/*.js _mocha -- src/**/*.spec.js",
    "check-coverage": "istanbul check-coverage coverage/coverage.json"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "color-convert": "^1.9.0",
    "event-emitter": "^0.3.5",
    "has-flag": "^2.0.0",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "react": "^16.0.0-alpha.12",
    "react-native": "^0.47.1",
    "react-native-checkbox": "^1.1.0",
    "react-native-image-picker": "^0.26.3",
    "react-native-message-bar": "^2.0.1",
    "react-native-simple-radio-button": "^2.6.0",
    "react-native-smart-splash-screen": "^2.3.4",
    "react-navigation": "^1.0.0-beta.12",
    "react-redux": "^5.0.5",
    "react-timer-mixin": "^0.13.3",
    "redux": "^3.7.2",
    "redux-api": "^0.11.0",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-loop": "^3.1.0",
    "redux-persist": "^4.8.3",
    "redux-persist-transform-filter": "^0.0.15",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "standard-http-error": "^2.0.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "20.0.3",
    "babel-polyfill": "^6.9.0",
    "babel-preset-react-native": "2.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^4.1.0",
    "chokidar-cli": "^1.2.0",
    "enzyme": "^2.2.0",
    "eslint": "^4.3.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-react": "^7.1.0",
    "fetch-mock": "^5.12.1",
    "istanbul": "1.0.0-alpha.2",
    "jest": "20.0.4",
    "mocha": "^3.5.0",
    "react-addons-test-utils": "15.6.0",
    "react-mixin": "^3.0.5",
    "react-native-mock": "~0.3.1",
    "react-test-renderer": "15.6.1",
    "rimraf": "^2.5.2",
    "sinon": "^3.0.0"
  }
}
